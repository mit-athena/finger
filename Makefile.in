# $Id: Makefile.in,v 1.1 1997-12-31 19:26:44 ghudson Exp $

SHELL=/bin/sh
INSTALL=@INSTALL@
INSTALL_PROGRAM=@INSTALL@
VPATH=@srcdir@
srcdir=@srcdir@
top_srcdir=@top_srcdir@
prefix=@prefix@
exec_prefix=@exec_prefix@
bindir=@bindir@

CC=@CC@
CPPFLAGS=@CPPFLAGS@
CFLAGS=@CFLAGS@
DEFS=@DEFS@
LDFLAGS=@LDFLAGS@
LIBS=-lhesiod -lzephyr @KRB4_LIBS@ @LIBS@ -lcom_err
ALL_CFLAGS=${DEFS} ${CPPFLAGS} ${CFLAGS}

all: finger

finger: finger.o
	${CC} ${LDFLAGS} -o $@ finger.o ${LIBS}

.c.o:
	${CC} -c ${ALL_CFLAGS} $<

install:
	${top_srcdir}/mkinstalldirs ${DESTDIR}${bindir}
	${INSTALL_PROGRAM} -m 555 finger ${DESTDIR}${bindir}

clean:
	rm -f finger finger.o

distclean: clean
	rm -f Makefile config.status config.cache config.log
